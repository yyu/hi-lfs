ock id="idm34100051584" text-align="start" wrap-option="no-wrap" keep-with-previous.within-column="always" space-before.optimum="0.6em" space-before.minimum="0.4em" space-before.maximum="0.8em" space-after.optimum="0.6em" space-after.minimum="0.4em" space-after.maximum="0.8em" hyphenate="false" white-space-collapse="false" white-space-treatment="preserve" linefeed-treatment="preserve" font-family="monospace" background-color="#E9E9E9" border-style="solid" border-width="0.5pt" border-color="#888" padding-start="5pt" padding-top="2pt" padding-bottom="2pt"><fo:inline font-weight="bold" font-family="monospace">make install</fo:inline></fo:block></fo:block><fo:block space-before.optimum="0.6em" space-before.minimum="0.4em" space-before.maximum="0.8em" orphans="3" widows="3">Move the <fo:inline font-family="monospace">xz</fo:inline> binary, and several symlinks
