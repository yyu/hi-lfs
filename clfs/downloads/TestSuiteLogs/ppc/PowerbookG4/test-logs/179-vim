[?25l[?1c:      [?25h[?0c:    else
[?25l[?1c:      [?25h[?0c:      $put ='OK ' . engine . ' - ' . pat
[?25l[?1c:      [?25h[?0c:    endif
[?25l[?1c:    [?25h[?0c:    if len(l) > 0
[?25l[?1c:      [?25h[?0c:"   check all the nine submatches
[?25l[?1c:      [?25h[?0c:      for i in range(1, 9)
[?25l[?1c:[8C[?25h[?0c:        if len(t) <= matchidx + i
