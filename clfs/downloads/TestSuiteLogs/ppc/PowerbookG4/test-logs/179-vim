STARTTEST
:" check for 'include' with \zs and no \ze
:call delete("./Xbase.c")
:call DeleteDirectory("Xdir1")
:!mkdir Xdir1
:!mkdir "Xdir1/dir2"
:let &include='^\s*%inc\s*\%([[:upper:]][^[:space:]]*\s\+\)\?\zs\S\+\ze'
:function! StripNewlineChar()
:  if v:fname =~ '\n$'
:    return v:fname[:-2]
:  endif
:  return v:fname
:endfunction
:let &includeexpr='StripNewlineChar()'
:e! Xdir1/dir2/foo.c
i%inc   bar.c[1m[34m^[[0m
:w
:e Xdir1/dir2/bar.c
i%inc      baz.c[1m[34m^[[0m
:w
:e Xdir1/dir2/baz.c
i%inc[12Cfoo.c[1m[34m^[[0m
:w
:e Xdir1/dir2/FALSE.c
i%inc[12Cfoo.c[1m[34m^[[0m
:w
:e Xbase.c
:set path=Xdir1/dir2
i%inc    FALSE.c foo.c[1m[34m^[[0m
:w
:redir! >>test.out
:checkpath!
:redir END
:brewind
:" change "\" to "/" for Windows and fix 'fileformat'
:e test.out
:%s#\\#/#g
:set ff&
:w
:q
ENDTEST
