STARTTEST
:" check for 'include' with \zs and \ze
:call delete("./Xbase.b")
:call DeleteDirectory("Xdir1")
:!mkdir Xdir1
:!mkdir "Xdir1/dir2"
:let &include='^\s*%inc\s*/\zs[^/]\+\ze'
:function! DotsToSlashes()
:  return substitute(v:fname, '\.', '/', 'g') . '.b'
:endfunction
:let &includeexpr='DotsToSlashes()'
:e! Xdir1/dir2/foo.b
i%inc   /bar/[1m[34m^[[0m
:w
:e Xdir1/dir2/bar.b
i%inc      /baz/[1m[34m^[[0m
:w
:e Xdir1/dir2/baz.b
i%inc[12C/foo/[1m[34m^[[0m
:w
:e Xbase.b
:set path=Xdir1/dir2
i%inc    /foo/[1m[34m^[[0m
:w
:redir! >>test.out
:checkpath!
:redir END
:brewind
ENDTEST
