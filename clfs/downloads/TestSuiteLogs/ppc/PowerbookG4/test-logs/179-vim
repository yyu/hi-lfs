h[?0ccall add(tl, [2, 'abc\@=', 'ababc', 'ab'])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0c" will never match, no matter the input text[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, 'abcd\@=e', 'abcd'])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0c" will never match[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, 'abcd\@=e', 'any text in here ... '])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, '\v(abc)@=..', 'xabcd', 'ab', 'abc'])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, '\(.*John\)\@=.*Bob', 'here is John, and here is B'])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, '\(John.*\)\@=.*Bob', 'John is Bobs friend', 'John is Bob', 'John is Bobs friend'])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, '\<\S\+\())\)\@=', '$((i=i+1))', 'i=i+1', '))'])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, '.*John\&.*Bob', 'here is John, and here is B'])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, '.*John\&.*Bob', 'John is Bobs friend', 'John is Bob'])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, '\v(test1)@=.*yep', 'this is a test1, yep it is', 'test1, yep', 'test1'])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, 'foo\(bar\)\@!', 'foobar'])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, 'foo\(bar\)\@!', 'foo bar', 'foo'])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, 'if \(\(then\)\@!.\)*$', ' if then else'])[?25l[?1c[28;1H[?25h[?0c[?25l[?1c[53;1H[K[53;1H:[?25h[?0ccall add(tl, [2, 'if \(\(then\)\@!.\)*$', ' if else ', 'if else ', ' '])[?25