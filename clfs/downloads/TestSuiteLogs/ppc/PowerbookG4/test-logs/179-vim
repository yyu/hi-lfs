:function TestHas()
  redir >> test.out
  for pl in ['6.9.999', '7.1.999', '7.4.123', '9.1.0', '9.9.1']
    echo 'has patch ' . pl . ': ' . has('patch-' . pl)
  endfor
  redir END
endfunc
:call TestHas()
